{
    "manifest_version": 2,
    "name": "Wiki Viewer",
    "version": "0.0.1",

    //"default_locale": "en",
    "description": "A extension that replaces your new tab page with a simple wikipedia search",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "permissions": [
        "system.storage",
        "system.memory",
        "webRequest",
        "*://*.wikipedia.org/*"
    ]

}