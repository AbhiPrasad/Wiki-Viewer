{
    "manifest_version": 2,
    "name": "Wiki Viewer",
    "version": "0.0.1",

    //"default_locale": "en",
    "description": "A extension that replaces your new tab page with a simple wikipedia search",
    //"icons":

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "permissions": [
        "system.storage",
        "system.memory",
        "webRequest",
        "*://*.wikipedia.org/*"
    ]

}